773999999996],[177.79554000000007,415.56773999999996]]},{"text":"","box":[[188.59284000000008,404.77043999999995],[233.5928400000001,404.77043999999995],[233.5928400000001,404.77043999999995],[188.59284000000008,404.77043999999995]]},{"text":"1","box":[[199.3901400000001,404.77043999999995],[210.1874400000001,404.77043999999995],[210.1874400000001,415.56773999999996],[199.3901400000001,415.56773999999996]]},{"text":"","box":[[210.1874400000001,404.77043999999995],[255.18744000000012,404.77043999999995],[255.18744000000012,404.77043999999995],[210.1874400000001,404.77043999999995]]},{"text":"月，他终于当上了总政治部主任。但是，","box":[[220.9847400000001,404.77043999999995],[415.3361400000001,404.77043999999995],[415.3361400000001,415.56773999999996],[220.9847400000001,415.56773999999996]]},{"text":"他这个总政治部主任，在任内","box":[[48.227940000000004,423.96563999999995],[188.59284000000002,423.96563999999995],[188.59284000000002,434.76293999999996],[48.227940000000004,434.76293999999996]]},{"text":"","box":[[188.59284000000008,423.96563999999995],[233.5928400000001,423.96563999999995],[233.5928400000001,423.96563999999995],[188.59284000000008,423.96563999999995]]},{"text":"1","box":[[199.3901400000001,423.96563999999995],[210.1874400000001,423.96563999999995],[210.1874400000001,434.76293999999996],[199.3901400000001,434.76293999999996]]},{"text":"","box":[[210.1874400000001,423.96563999999995],[255.18744000000012,423.96563999999995],[255.18744000000012,423.96563999999995],[210.1874400000001,423.96563999999995]]},{"text":"年又9个月，几乎没有到过总政治","box":[[220.9847400000001,423.96563999999995],[382.9442400000001,423.96563999999995],[382.9442400000001,434.76293999999996],[220.9847400000001,434.76293999999996]]},{"text":"部机关。总政治部的干部大多数没有见过这位主任。","box":[[48.227940000000004,443.40077999999994],[296.56584,443.40077999999994],[296.56584,454.19807999999995],[48.227940000000004,454.19807999999995]]},{"text":"那么，张春桥在总政治部干过什么呢？他上任伊始，就把矛头","box":[[72.22194,462.83591999999993],[376.7058,462.83591999999993],[376.7058,474.1130999999999],[72.22194,474.1130999999999]]},{"text":"对着老干部，对着老一代革命家。1975","box":[[48.227940000000004,481.79117999999994],[257.9355,481.79117999999994],[257.9355,492.82841999999994],[48.227940000000004,492.82841999999994]]},{"text":"","box":[[257.9355,481.79117999999994],[303.9355,481.79117999999994],[303.9355,481.79117999999994],[257.9355,481.79117999999994]]},{"text":"年3","box":[[268.97274,481.79117999999994],[291.04722,481.79117999999994],[291.04722,492.82841999999994],[268.97274,492.82841999999994]]},{"text":"","box":[[291.04722,481.79117999999994],[337.04722,481.79117999999994],[337.04722,481.79117999999994],[291.04722,481.79117999999994]]},{"text":"月","box":[[302.08446,481.79117999999994],[313.1217,481.79117999999994],[313.1217,492.82841999999994],[302.08446,492.82841999999994]]},{"text":"","box":[[313.1217,481.79117999999994],[359.1217,481.79117999999994],[359.1217,481.79117999999994],[313.1217,481.79117999999994]]},{"text":"1","box":[[324.15894,481.79117999999994],[335.19617999999997,481.79117999999994],[335.19617999999997,492.82841999999994],[324.15894,492.82841999999994]]},{"text":"","box":[[335.19617999999997,481.79117999999994],[381.19617999999997,481.79117999999994],[381.19617999999997,481.79117999999994],[335.19617999999997,481.79117999999994]]},{"text":"日，他在全军各","box":[[346.23341999999997,481.79117999999994],[423.49409999999995,481.79117999999994],[423.49409999999995,492.82841999999994],[346.23341999999997,492.82841999999994]]},{"text":"大单位政治部主任座谈会上，借毛泽东关于理论问题的谈话，大讲","box":[[48.707820000000005,500.74643999999995],[361.82952,500.74643999999995],[361.82952,511.54373999999996],[48.707820000000005,511.54373999999996]]},{"text":"反对经验主义，说“十多年来的事实证明，经验主义是修正主义","box":[[48.46788000000001,519.94164],[357.51059999999995,519.94164],[357.51059999999995,530.97888],[48.46788000000001,530.97888]]},{"text":"的助手”，明显地攻击重新工作、具有丰富经验的老干部、老革命","box":[[49.18770000000001,538.8969],[355.35114,538.8969],[355.35114,549.45426],[49.18770000000001,549.45426]]},{"text":"家。直到邓小平向毛泽东反映，5","box":[[48.707820000000005,558.0921],[210.66732,558.0921],[210.66732,568.8893999999999],[48.707820000000005,568.8893999999999]]},{"text":"","box":[[210.6673200000001,558.0921],[255.66732000000013,558.0921],[255.66732000000013,558.0921],[210.6673200000001,558.0921]]},{"text":"月","box":[[221.4646200000001,558.0921],[232.26192000000012,558.0921],[232.26192000000012,568.8893999999999],[221.4646200000001,568.8893999999999]]},{"text":"","box":[[232.26192000000012,558.0921],[277.26192000000015,558.0921],[277.26192000000015,558.0921],[232.26192000000012,558.0921]]},{"text":"3","box":[[243.05922000000012,558.0921],[253.85652000000013,558.0921],[253.85652000000013,568.8893999999999],[243.05922000000012,568.8893999999999]]},{"text":"","box":[[253.85652000000013,558.0921],[298.85652000000005,558.0921],[298.85652000000005,558.0921],[253.85652000000013,558.0921]]},{"text":"日，毛泽东在中央政治局会","box":[[264.6538200000001,558.0921],[394.2214200000001,558.0921],[394.2214200000001,568.8893999999999],[264.6538200000001,568.8893999999999]]},{"text":"议上，对江青等人再次作了批评：“我看批经验主义的人，","box":[[48.46788000000001,577.5272399999999],[335.43611999999996,577.5272399999999],[335.43611999999996,588.5644799999999],[48.46788000000001,588.5644799999999]]},{"text":"","box":[[335.43611999999996,577.5272399999999],[381.43611999999996,577.5272399999999],[381.43611999999996,577.5272399999999],[335.43611999999996,577.5272399999999]]},{"text":"自己就","box":[[346.47335999999996,577.5272399999999],[379.58507999999995,577.5272399999999],[379.58507999999995,588.5644799999999],[346.47335999999996,588.5644799999999]]},{"text":"3B9","box":[[381.02472,597.6822],[401.17968,597.6822],[401.17968,604.4005199999999],[381.02472,604.4005199999999]]}]}